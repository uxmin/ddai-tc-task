{
  "name": "tc-task",
  "displayName": "TC Task",
  "publisher": "uxmin",
  "repository": {
    "type": "git",
    "url": "https://github.com/uxmin/ddai-tc-task.git"
  },
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "my-task-view-container": [
        {
          "id": "myTaskFiles",
          "name": "Assigned Files"
        }
      ]
    },
    "commands": [
      {
        "command": "tc-task.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.openReviewPanel",
        "title": "Open Review Panel"
      }
    ],
    "fileDecorations": [
      {
        "scheme": "file",
        "pattern": "**/TC_*.json"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "my-task-view-container",
          "title": "My Tasks",
          "icon": "media/shield.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "xlsx": "^0.18.5"
  }
}